  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Serverless",
      "program": "${workspaceFolder}/node_modules/serverless/bin/serverless",
      "args": [
        "offline",
        "start",
        "--stage",
        "development"
      ],
      "env": {
        "SLS_DEBUG": "*"
      }
    }
  ]
